{
    "parser": "@typescript-eslint/parser",
    "plugins": ["@typescript-eslint", "import"],
    "extends": [
        "prettier",
        "typescript",
        "typescript/prettier",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:import/typescript"
    ],
    "rules": {
        "prettier/prettier": "error",
        "import/no-unresolved": "error",
        "import/extensions": "off",
        "require-jsdoc": 1
    }
}